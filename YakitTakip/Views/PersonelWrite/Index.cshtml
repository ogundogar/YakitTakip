@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model IQueryable<YakitTakip.Models.TbPersonel>
<main>
    <h3>Personel</h3>
    <div class="container">
        <div class="row">
            <div class="bg-danger col-md-3 col-sm-12">
                @Html.Partial("../Shared/PartialViews/aside")
            </div>
            <section class="col-md-9 col-sm-12">
                <article>
                    <form asp-action="Index" asp-controller="PersonelAdd" method="post">

                        <label for="ad">Ad:</label><br>
                        <input type="text" id="ad" name="ad"><br>

                        <label for="soyad">Soyad:</label><br>
                        <input type="text" id="soyad" name="soyad"><br><br>

                        <label for="telefon">Telefon Numarası:</label><br>
                        <input type="text" id="telefon" name="telefon"><br><br>

                        <input type="submit" value="Submit">
                    </form>
                </article>
            </section>
        </div>
    </div>
</main>